if __name__ == '__main__':
    a = int(input())  # Read the first integer
    b = int(input())  # Read the second integer
    
    # Print the sum of a and b
    print(a + b)
    
    # Print the difference of a and b (a - b)
    print(a - b)
    
    # Print the product of a and b
    print(a * b)
