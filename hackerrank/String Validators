def string_validation(s):
    # Check if string contains alphanumeric characters
    print(any(char.isalnum() for char in s))
    # Check if string contains alphabetical characters
    print(any(char.isalpha() for char in s))
    # Check if string contains digits
    print(any(char.isdigit() for char in s))
    # Check if string contains lowercase characters
    print(any(char.islower() for char in s))
    # Check if string contains uppercase characters
    print(any(char.isupper() for char in s))

# Read input
if __name__ == '__main__':
    s = input()
    
    # Call the function to validate the string
    string_validation(s)
