# Input the dimensions
N, M = map(int, input().split())

# Top part of the design
for i in range(1, N, 2):
    print(('.|.' * i).center(M, '-'))

# Center line with 'WELCOME'
print('WELCOME'.center(M, '-'))

# Bottom part of the design
for i in range(N-2, 0, -2):
    print(('.|.' * i).center(M, '-'))
