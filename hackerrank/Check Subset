# Number of test cases
t = int(input())

# Loop through each test case
for _ in range(t):
    # Read the first set
    n = int(input())
    set_A = set(map(int, input().split()))
    
    # Read the second set
    m = int(input())
    set_B = set(map(int, input().split()))
    
    # Check if set_A is a subset of set_B
    if set_A.issubset(set_B):
        print(True)
    else:
        print(False)
